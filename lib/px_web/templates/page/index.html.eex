<style>
	:root {
		--primary-color: #55FF99;
		--secondary-color: green;

		--bg-color: #fff;
		--font-color: #333;
	}

	body {
		margin: 0;
		font-family: Roboto, sans-serif;
		background-color: var(--bg-color);
	}

	nav {
		width: 100%;
		display: flex;
		flex-direction: row;
		justify-content: space-between;

		background-color: transparent;
		border-style: solid;
		border-width: 0 0 0.5vh 0;
		border-color: var(--primary-color);
		font-size: 2.2vh;
	}

	nav > ul {
		list-style: none;
		display: flex;
		flex-direction: row;
		margin: 0;
	}

	nav > ul > a > li {
		padding: 2vh 1vw;
		color: var(--font-color);
	}

	nav > ul > a > li:hover {
		background-color: var(--primary-color);
	}

	a {
		text-decoration: none;
		color: inherit;
	}

	main {
		margin-top: 5vh;
	}

	section {
		display: flex;
		flex-direction: row;
		justify-content: space-around;
		align-items: center;
	}

	section > div > h1 {
		font-size: 4vw;
		margin: 0;
	}

	section > div > h3 {
		margin: 0;
	}

	h1,
	h3 {
		color: var(--font-color);
	}

	svg {
		max-height: 50vh;
		max-width: 50vw;
	}

	footer {
		width: 100%;
		height: 25vh;
		padding: 1vh 0;
		display: flex;
		flex-direction: column;
		justify-content: space-between;
		background-color: #eee;
	}

	footer > div {
		display: flex;
		flex-direction: row;
		justify-content: space-around;
	}

	footer > div > ul {
		list-style: none;
	}

	footer > div > ul > li > h4 {
		margin: 0;
	}

	.divider {
		width: 100%;
		height: 0;
		margin-top: 1vh;
		margin-bottom: 1vh;
	}

	#logo {
		max-height: 4.5vh;
		padding: 1vh;
	}
</style>

